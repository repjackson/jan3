template(name='block')
    if can_view_block
        .ui(class=block_classes)
            if view_title
                .ui.header
                    if icon
                        +icon name=icon
                    |#{title}
            if custom_template
                +Template.dynamic template=custom_template
            else
                if is_cards
                    .ui.centered.three.cards
                        each children
                            .ui.card
                                if title
                                    .content
                                        a.grey.header(href="/v/#{_id}")
                                            if icon
                                                +icon name=icon
                                            |#{title} 
                                .content
                                    .ui.list
                                        each ../fields
                                            .item
                                                if field_template
                                                    +Template.dynamic template=field_template data=..
                                                else
                                                    | #{child_schema_field_value}
                                .content
                                    each ../actions
                                        .ui.icon.button(title=title)
                                            i.icon
                                                +icon name=icon
                                    // +edit_button
                                    // +view_button

                if is_table
                    if view_table_header
                        .middle.aligned.row
                            +table_header doc_type=children_doc_type stat_type=table_stat_type
                    table.ui.striped.selectable.fixed.table
                        thead
                            tr
                                each fields
                                    if sortable
                                        th.sort_by
                                            |#{label}
                                            if sort_descending
                                                i.chevron.down.link.blue.icon
                                            else if sort_ascending
                                                i.chevron.up.link.blue.icon
                                            else
                                                i.sort.link.grey.icon
                                    else
                                        th
                                            |#{label}
                        tbody
                            each children
                                tr
                                    each ../fields
                                        if field_template
                                            td 
                                                +Template.dynamic template=field_template data=..
                                        else
                                            td(class=table_cell_class) #{child_schema_field_value}
                                        
                        if view_table_footer
                            +table_footer columns=fields.length doc_type=children_doc_type stat_type=table_stat_type
                if is_list
                    .ui.divided.list
                        each children
                            .item
                                .ui.horizontal.list
                                    each ../fields
                                        .item
                                            if field_template
                                                +Template.dynamic template=field_template data=..
                                            else
                                                | #{child_schema_field_value}
                if is_grid
                    .ui.equal.width.four.column.grid
                        each children
                            .column
                                .ui.secondary.segment
                                    if title
                                        a.ui.header(href="/p/#{slug}")
                                            if icon
                                                i.icon  
                                                    +icon name=icon
                                            |#{title}    
                                    .ui.list
                                        each ../fields
                                            .item
                                                if field_template
                                                    +Template.dynamic template=field_template data=..
                                                else
                                                    | #{child_schema_field_value}
                if is_comments
                    .ui.comments
                        each children
                            +doc_history_event
                if is_header
                    .centered.middle.aligned.row
                        .three.wide.center.aligned.column
                            if is_admin
                                +link_button link='/p/admin' icon='user-shield' label='Admin'
                        .ten.wide.column
                            h2.ui.center.aligned.grey.header 
                                +icon name='code'
                                |Dev
                        .three.wide.right.aligned.column
            if editing_mode
                .ui.tertiary.segment
                    h3.ui.header 
                        +icon name=icon
                        |Edit #{title} #{_id}
                    i.remove_block.remove.icon
                    +set_key_value key='published' value=true label='Published'
                    +set_key_value key='published' value=false label='Hidden'
                    +set_key_value key='view_title' value=true label='View Title'
                    +set_key_value key='view_title' value=false label='Hide Title'
                    .ui.header Rank
                    +set_key_value key='rank' value=1 label='1'
                    +set_key_value key='rank' value=2 label='2'
                    +set_key_value key='rank' value=3 label='3'
                    +set_key_value key='rank' value=4 label='4'
                    +set_key_value key='rank' value=5 label='5'

                    .ui.form
                        .fields
                            +edit_block_text_field label='Title' key='title'
                            +edit_block_text_field label='Icon' key='icon'
                            +edit_block_text_field label='Classes' key='block_classes'
                        .fields
                            +edit_block_text_field label='Slug' key='slug'
                            +edit_block_text_field label='Parent Slug' key='parent_slug'
                            +edit_block_text_field label='Custom Template' key='custom_template'
                    unless custom_template
                        h3.ui.header
                            i.filter.icon
                            |Filter
                        .ui.form
                            +edit_block_text_field label='Hard Limit' key='hard_limit'
                            +edit_block_text_field label='Soft Limit' key='limit'
                            h5 
                                |Status
                            +set_key_value key='filter_status' value="ACTIVE" label='Active Status'
                            +set_key_value key='filter_status' value="" label='All'
                            h5.ui.header 
                                i.key.icon
                                |Key
                            +set_key_value key='filter_key' value='_id' label='_id'
                            +set_key_value key='filter_key' value='ev.FRANCHISEE' label='ev.FRANCHISEE'
                            +set_key_value key='filter_key' value='ev.CUSTOMER' label='ev.CUSTOMER'
                            +set_key_value key='filter_key' value='ev.MASTER_LICENSEE' label='ev.MASTER_LICENSEE'
                            +set_key_value key='filter_key' value='office_jpid' label='office_jpid'
