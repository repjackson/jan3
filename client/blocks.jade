        .content
            // .right.floated.meta $#{price}
            a.header(href="/v/#{_id}") #{title}
            if is_admin
                .meta #{slug}
            .description
                p #{description}
        // if request_count
        //     .content
        //         | #{request_count} requests
        .content
            +request_service_button
            a.ui.button(href="/v/#{_id}") 
                i.large.icon
                    +iconsmall name='view-details'
                |Details
     
     
     
        
